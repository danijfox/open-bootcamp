public class Main{

    public static void main (String[] arg){

        int number1 = 3;
        int number2 = 5;
        int number3 = 7;

        var valor = suma(number1, number2, number3);

        System.out.println(valor);

    }

    public static int suma(int a, int b, int c){
        return a+b+c;
    }
}
